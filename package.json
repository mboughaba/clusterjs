{
  "name": "clusterjs",
  "version": "0.0.2",
  "description": "simple nodejs cluster implementation",
  "scripts": {
    "test": "mocha",
    "test-watch": "mocha --opts ./test/mocha-watch.opts",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha --",
    "lint": "jshint .",
    "check": "jscs .",
    "inspect": "jsinspect .",
    "coverage": "istanbul check-coverage --statement 100 --branch 100 --function 100 --line 100"
  },
  "keywords": [
      "simple",
      "nodejs",
    "cluster",
    "node",
    "core",
    "multiple"
  ],
  "author": "Mohamed Boughaba <mohamed.bgb@gmail.com>",
  "private": true,
  "dependencies": {
    "debug": "^2.2.0",
    "yargs": "^3.11.0"
  },
  "devDependencies": {
    "istanbul": "^0.3.15",
    "mocha": "^2.2.5",
    "precommit-hook": "^2.0.1",
    "rewire": "^2.3.4",
    "should": "^6.0.3",
    "sinon": "^1.15.3",
    "supertest": "^1.0.1"
  },
  "pre-commit": [
    "lint",
    "check",
    "inspect",
    "cover",
    "coverage"
  ]
}
